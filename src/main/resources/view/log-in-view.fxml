<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.marcin.jacek.polewski.Task_Manager_Project.controller.LogInViewController">
   <children>
      <VBox fx:id="vBox" maxHeight="300.0" maxWidth="300.0" StackPane.alignment="CENTER">
         <children>
            <Label fx:id="promptLabel" text="Please select user, or create one" wrapText="true" />
            <Label fx:id="signUpLabel" text="Label" wrapText="true" />
            <VBox fillWidth="false">
               <children>
                  <HBox alignment="CENTER_RIGHT" fillHeight="false" prefHeight="20.0" prefWidth="300.0">
                     <children>
                        <Button fx:id="editUserListButton" mnemonicParsing="false" onAction="#editUsersPressed" text="edit" />
                        <Button fx:id="addUserToListButton" mnemonicParsing="false" onAction="#addUserListButtonPressed" text="+" />
                     </children>
                  </HBox>
                  <ScrollPane fx:id="userScrollView" fitToHeight="true" fitToWidth="true" prefHeight="174.0" prefWidth="300.0">
                     <content>
                        <VBox fx:id="vboxWithUsers" prefHeight="27.0" prefWidth="100.0" />
                     </content>
                  </ScrollPane>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</StackPane>
